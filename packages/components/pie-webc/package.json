    "@justeattakeaway/pie-button": "0.45.5",
    "@justeattakeaway/pie-modal": "0.38.7",
    "@justeattakeaway/pie-webc-core": "0.19.0"
  },
  "volta": {
    "extends": "../../../package.json"
  },
  "sideEffects": [
    "dist/*.js"
  ]
