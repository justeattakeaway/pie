  },
  "keywords": [
    "PIE"
  ],
  "scripts": {
    "build": "vite build && tsc",
    "build:icons": "node ./build.js",
    "clean": "run -T rimraf -rf ./dist/ ./icons/",
    "lint:scripts": "run -T eslint .",
    "lint:scripts:fix": "yarn lint:scripts --fix",
    "test:browsers-setup": "npx playwright-lit-setup",
