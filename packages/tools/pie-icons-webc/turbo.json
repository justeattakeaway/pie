{
  "$schema": "https://turborepo.org/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build:icons": {
      "cache": true,
      "dependsOn": [
        "@justeattakeaway/pie-icons#build",
        "clean"
      ],
      "outputs": [
        "icons/**"
      ]
    },
    "build": {
      "cache": true,
      "dependsOn": [
        "^build",
        "build:icons"
      ],
      "inputs": [
        "icons/**"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test:browsers": {
      "cache": false,
      "dependsOn": [
        "^build",
        "test:browsers-setup"
      ],
      "outputs": [
        "__snapshots__/**"
      ]
    },
    "test:browsers:ci": {
      "cache": false,
      "dependsOn": [
        "^build",
        "test:browsers-setup"
      ]
    }
  }
}
