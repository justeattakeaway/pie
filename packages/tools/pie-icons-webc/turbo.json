{
    "$schema": "https://turborepo.org/schema.json",
    "extends": ["//"],
    "pipeline": {
      "clean": {
        "cache": false,
        "dependsOn": []
      },
      "build:icons": {
        "cache": false,
        "dependsOn": [
          "clean"
        ],
        "outputs": [
          "icons"
        ]
      },
      "build": {
        "cache": true,
        "dependsOn": [
          "^build",
          "build:icons"
        ],
        "outputs": [
          "dist"
        ]
      }
    }
  }
