  "keywords": [
    "PIE",
    "yeoman-generator"
  ],
  "dependencies": {
    "chalk": "5.3.0",
    "yeoman-generator": "5.9.0"
  },
  "devDependencies": {
    "@types/yeoman-generator": "5.2.11",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-delete": "2.0.0"
  },
  "repository": "https://github.com/justeattakeaway/pie.git",
  "license": "Apache-2.0"
}
