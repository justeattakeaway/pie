{
  "compilerOptions": {
      "module": "ESNext",
      "target": "es6",
      "lib": ["es6"],
      "esModuleInterop": true,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "paths": {
        "@/*": ["./src/*"]
      },
      "strictNullChecks": true,
      "sourceMap": false,
      "skipLibCheck": true
  },
  "exclude": [
      "node_modules",
      "src/app/templates",
      "generators"
  ]
}