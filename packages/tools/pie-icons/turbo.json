{
  "$schema": "https://turborepo.org/schema.json",
  "extends": [
    "//"
  ],
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build",
        "clean"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "bin/**",
        "src/**"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
