{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/master",
    "pipeline": {
        "build": {
            "dependsOn": ["^build"]
        },
        "test": {
            "dependsOn": ["build"],
            "outputs": [],
            "inputs": ["src/**"]
        },
        "lint": {
            "outputs": []
        },
        "lint:style": {
          "cache": false,
          "dependsOn": []
        },
        "deploy": {
            "dependsOn": [],
            "outputs": [],
            "cache": false
        },
        "dev": {
            "cache": false,
            "dependsOn": []
        }
    }
}
