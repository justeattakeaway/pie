{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:examples": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "build/**",
        "dist/**",
        ".next/**",
        ".nuxt/**",
        ".output/**"
      ]
    },
    "watch": {
      "cache": true,
      "dependsOn": []
    },
    "pie-docs#build": {
      "cache": true,
      "env": [
        "PIE_URL_PREFIX"
      ],
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "pie-docs#build:dev": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "pie-docs#dev": {
      "cache": false,
      "dependsOn": [
        "pie-docs#build:dev",
        "^build"
      ]
    },
    "clean": {
      "cache": true
    },
    "pie-storybook#serve": {
      "dependsOn": [
        "pie-storybook#build"
      ]
    },
    "pie-storybook#dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "pie-docs#test:generate-routes": {
      "dependsOn": [
        "pie-docs#build"
      ],
      "cache": false
    },
    "test": {
      "cache": true,
      "dependsOn": [
        "build",
        "^test"
      ],
      "outputs": [
        "**/__snapshots__/**",
        "**/snapshots/**"
      ],
      "inputs": [
        "src/**"
      ]
    },
    "test:ci": {
      "cache": true,
      "dependsOn": [
        "build",
        "^test:ci"
      ],
      "outputs": [
        "**/__snapshots__/**",
        "**/snapshots/**"
      ],
      "inputs": [
        "src/**"
      ]
    },
    "test:coverage": {
      "outputs": []
    },
    "test:browsers": {
      "cache": true,
      "inputs": [
        "src/**",
        "test/**"
      ],
      "outputs": []
    },
    "test:browsers:ci": {
      "cache": true,
      "env": [
        "PIE_URL_PREFIX"
      ],
      "inputs": [
        "src/**",
        "test/**"
      ]
    },
    "test:visual": {
      "cache": false,
      "outputs": []
    },
    "test:visual:ci": {
      "cache": false
    },
    "lint:scripts": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint:scripts:fix": {
      "cache": false,
      "outputs": []
    },
    "lint:style": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": []
    }
  }
}
