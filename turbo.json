{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "pie-docs#build": {
      "cache": true,
      "dependsOn": [
        "^build",
        "@justeattakeaway/pie-icons#build"
      ]
    },
    "pie-docs#build:dev": {
      "cache": true,
      "dependsOn": [
        "^build",
        "@justeattakeaway/pie-icons#build"
      ]
    },
    "pie-docs#dev": {
      "cache": false,
      "dependsOn": [
        "pie-docs#build:dev",
        "^build"
      ]
    },
    "clean": {
      "cache": true
    },
    "pie-storybook#serve": {
      "dependsOn": [
        "pie-storybook#build"
      ]
    },
    "pie-storybook#dev": {
      "cache": false,
      "dependsOn": [
        "^build"
      ]
    },
    "test:generate-routes": {
      "cache": false
    },
    "test": {
      "cache": true,
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "inputs": [
        "src/**"
      ]
    },
    "test:ci": {
      "cache": true,
      "outputs": [],
      "inputs": [
        "src/**"
      ]
    },
    "test:coverage": {
      "outputs": []
    },
    "test:system": {
      "cache": false,
      "outputs": []
    },
    "test:system:ci": {
      "cache": false
    },
    "test:visual": {
      "cache": false,
      "outputs": []
    },
    "test:visual:ci": {
      "cache": false
    },
    "lint:scripts": {
      "cache": true,
      "outputs": []
    },
    "lint:scripts:fix": {
      "outputs": []
    },
    "lint:style": {
      "cache": true,
      "dependsOn": []
    },
    "dev": {
      "cache": false,
      "dependsOn": []
    }
  }
}
