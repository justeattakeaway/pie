{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/master",
    "pipeline": {
        "build": {
            "cache": true,
            "dependsOn": ["^build"]
        },
        "test": {
            "cache": true,
            "dependsOn": ["build"],
            "outputs": [],
            "inputs": ["src/**"]
        },
        "test:coverage": {
          "outputs": []
        },
        "lint": {
            "cache": true,
            "outputs": []
        },
        "lint:fix": {
          "outputs": []
        },
        "lint:style": {
          "cache": true,
          "dependsOn": []
        },
        "dev": {
            "cache": false,
            "dependsOn": []
        }
    }
}
