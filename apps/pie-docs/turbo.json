{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      "cache": true,
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:dev": {
      "cache": true,
      "dependsOn": [
        "^build"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "build:dev",
        "^build"
      ]
    },
    "test:generate-routes": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    }
  }
}
