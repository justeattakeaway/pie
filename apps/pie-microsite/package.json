{
  "name": "pie-microsite",
  "private": true,
  "description": "todo",
  "version": "1.50.0",
  "main": "index.js",
  "keywords": [],
  "author": "JustEatTakeaway - Vue Design System Team",
  "license": "Apache-2.0",
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "build": "DEPLOYMENT_ENVIRONMENT='production' eleventy",
    "build:dev": "DEPLOYMENT_ENVIRONMENT='development' eleventy",
    "clean:dist": "run -T rimraf dist",
    "dev": "yarn clean:dist && yarn build:dev --serve",
    "lint:scripts": "run -T eslint .",
    "lint:scripts:fix": "run -T eslint . --fix",
    "lint:style": "run -T stylelint ./src/**/*.{css,scss}",
    "lint:style:fix": "yarn lint:style --fix",
    "test": "run -T jest",
    "test:ci": "yarn test",
    "test:generate-routes": "node test/helpers/generate-routes.js",
    "test:system": "run -T cross-env-shell TEST_TYPE=system wdio run ../../wdio.conf.js --spec ./test/system/*.js",
    "test:system:ci": "yarn test:system",
    "test:visual": "run -T cross-env-shell PERCY_TOKEN=${PERCY_TOKEN_PIE_MICROSITE} TEST_TYPE=visual percy exec -- wdio ../../wdio.conf.js --spec ./test/visual/*.js",
    "test:visual:ci": "yarn test:visual"
  },
  "browserslist": [
    "extends @justeat/browserslist-config-fozzie"
  ],
  "devDependencies": {
    "@11ty/eleventy": "1.0.2",
    "@11ty/eleventy-navigation": "0.3.5",
    "dree": "3.4.2",
    "eleventy-plugin-clean": "1.1.3",
    "eleventy-plugin-rev": "1.0.2",
    "eleventy-sass": "2.1.3",
    "markdown-it-attrs": "4.1.6"
  },
  "dependencies": {
    "markdown-it": "13.0.1"
  }
}
