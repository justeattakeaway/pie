<nav aria-label="Secondary navigational tabs" class="c-tabs">
    <ul>
        {%- for entry in collections.all | eleventyNavigation(navKey) %}
        <li
            {% if entry.url == page.url %}
                aria-selected="true"
            {% else %}
                aria-selected="false"
            {% endif %}
            class="c-tabs-item">
                <a href="{{ entry.url | url }}" class="c-tabs-link {% if entry.url == page.url %}is-active{% endif %}">{{ entry.title }}</a></li>
        {%- endfor %}
    </ul>
</nav>
