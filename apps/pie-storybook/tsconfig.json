{
  "compilerOptions": {
      "module": "CommonJS",
      "target": "es6",
      "lib": ["es6", "dom"],
      "esModuleInterop": true,
      "moduleResolution": "node",
      "noImplicitAny": true,
      "strictNullChecks": true,
      "sourceMap": false,
      "skipLibCheck": true,
      "resolveJsonModule": true
  },
  "exclude": [
      "node_modules",
  ]
}
