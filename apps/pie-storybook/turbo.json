{
  "$schema": "https://turborepo.org/schema.json",
  "extends": [
    "//"
  ],
  "pipeline": {
    "generate:design-tokens": {
      "cache": true,
      "outputs": [
        "stories/design-tokens.mdx"
      ]
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^build",
        "^generate:component-statuses",
        "generate:design-tokens"
      ]
    },
    "dev:testing": {
      "cache": false,
      "dependsOn": [
        "^build",
        "^generate:component-statuses"
      ]
    },
    "build": {
      "cache": true,
      "dependsOn": [
        "^build",
        "^generate:component-statuses",
        "generate:design-tokens"
      ],
      "outputs": ["dist/**"]
    },
    "build:testing": {
      "cache": true,
      "dependsOn": [
        "^build",
        "^generate:component-statuses"
      ],
      "outputs": ["dist/**"]
    },
    "test": {
      "cache": true,
      "dependsOn": []
    }
  }
}
