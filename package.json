{
  "name": "pie",
  "description": "JustEatTakeaway",
  "version": "1.16.1",
  "keywords": [],
  "author": "JustEatTakeaway - Vue Design System Team",
  "license": "MIT",
  "packageManager": "yarn@3.2.1",
  "engines": {
    "node": "^16"
  },
  "workspaces": [
    "apps/**/*",
    "packages/**/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "test:ci": "turbo run test:ci",
    "test:coverage": "turbo run test:coverage",
    "lint:scripts": "turbo run lint:scripts",
    "lint:scripts:fix": "turbo run lint:fix",
    "lint:style": "turbo run lint:style --continue",
    "dev": "turbo run dev",
    "postinstall": "husky install",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable"
  },
  "stylelint": {
    "extends": "@justeat/stylelint-config-fozzie"
  },
  "devDependencies": {
    "@babel/cli": "7.19.3",
    "@babel/core": "7.17.10",
    "@babel/eslint-parser": "7.19.1",
    "@babel/node": "7.16.8",
    "@babel/preset-env": "7.16.11",
    "@justeat/browserslist-config-fozzie": "1.x",
    "@justeat/eslint-config-fozzie": "5.1.0",
    "@justeat/stylelint-config-fozzie": "3.x",
    "autoprefixer": "10.4.7",
    "babel-loader": "8.2.3",
    "cssnano": "5.1.13",
    "eslint": "8.14.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "jest": "27.5.1",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "postcss": "8.4.16",
    "postcss-cli": "10.0.0",
    "sass": "1.54.0",
    "stylelint": "14.9.1",
    "stylelint-config-standard-scss": "5.0.0",
    "turbo": "1.3.1",
    "vue": "2.6.10",
    "vue-template-compiler": "2.6.10"
  },
  "dependencies": {
    "@justeat/fozzie": "10.0.0",
    "@justeattakeaway/pie-icons": "1.0.0"
  }
